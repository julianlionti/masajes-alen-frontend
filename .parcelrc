{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.{ts,tsx}": [
      "@parcel/transformer-typescript-tsc"
    ],
    "*.{mp4,svg}": [
      "@parcel/transformer-raw"
    ]
  },
  "optimizers": {
    "*.css": [
      "@parcel/optimizer-cssnano"
    ],
    "*.html": [
      "@parcel/optimizer-htmlnano"
    ],
    "*.{png,jpg,jpeg,svg,...}": [
      "@parcel/optimizer-imagemin"
    ]
  }
}
